
package com.socgen.unibank.bridge.model.responses;

import lombok.Getter;
import lombok.Setter;

@Getter
@Setter
public class AmortizableLoanResponse {
    
    // Identifiants du prêt créé
    private String fileNumber;
    private String orderNumber;
    private String amendmentNumber;
    
    // Informations de la branche
    private String branchCode;
    private String branchDesignation;
    
    // Statut de la réponse
    private String statusCode; // 0, 1, ERROR
    private String statusMessage;
    
    // Métadonnées de la réponse
    private String requestId;
    private String responseId;
    private String timestamp;
    private String serviceVersion;
    private String languageCode;
    private String languageDesignation;
}
